                    <title xml:id="title_bfdd631f" xml:lang="en">Six Songs to Texts by Ludvig Holstein</title>
                    <respStmt xml:id="respStmt_d4525231">
                        <persName authURI="http://www.viaf.org" authority="VIAF" codedval="197250" role="composer" xml:id="persName_id4e8f7114">Carl Nielsen</persName>
